/* Hauria de funcionar (NO PROVAT)
    CÃ lcul de nombres primers. Omple l'array 'primers' dels 'num' nombres primers.
*/

func modul(int num, int div) : int begin
    while (num  >= div) do
        num = num - div;
    end;
    return num;
end llegirEnter;


proc main() begin
	int num = 10;
	int[num] primers;
	int n = 1;
	primers[0] = 1;
	
	for (int i = 2; i < num; i++) do
        boolean primer = true;
	    for (int j = 2; j < n-1; j++) do
	        if modul(j, primers[j]) == 0 do
	            primer = false;
	        end;
	    end;
	    if primer == true do
	        primers[n] = i;
	        n++;
	    end;
    end;
end main;
